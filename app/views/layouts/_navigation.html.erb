<%= render 'modals/contact' %>
<%= render 'modals/register' %>
<%= render 'modals/login' %>
<!--Double navigation-->
   <header>
       <!-- Sidebar navigation -->
       <ul id="slide-out" class="side-nav fixed sn-bg-1 custom-scrollbar">
           <!-- Logo -->
           <li>
               <div class="logo-wrapper waves-light wow rubberBand animated">
                 <p id="icon" style="text-shadow: rgb(26, 51, 52) 0px 0px 0px, rgb(26, 52, 52) 1px 1px 0px, rgb(27, 52, 53) 2px 2px 0px, rgb(27, 53, 54) 3px 3px 0px, rgb(27, 54, 54) 4px 4px 0px, rgb(27, 54, 55) 5px 5px 0px, rgb(28, 55, 56) 6px 6px 0px, rgb(28, 55, 56) 7px 7px 0px, rgb(28, 56, 57) 8px 8px 0px, rgb(29, 57, 58) 9px 9px 0px, rgb(29, 57, 58) 10px 10px 0px, rgb(29, 58, 59) 11px 11px 0px, rgb(30, 59, 59) 12px 12px 0px, rgb(30, 59, 60) 13px 13px 0px, rgb(30, 60, 61) 14px 14px 0px, rgb(31, 60, 61) 15px 15px 0px, rgb(31, 61, 62) 16px 16px 0px, rgb(31, 62, 63) 17px 17px 0px, rgb(32, 62, 63) 18px 18px 0px, rgb(32, 63, 64) 19px 19px 0px, rgb(32, 64, 64) 20px 20px 0px, rgb(33, 64, 65) 21px 21px 0px, rgb(33, 65, 66) 22px 22px 0px, rgb(33, 65, 66) 23px 23px 0px, rgb(34, 66, 67) 24px 24px 0px, rgb(34, 67, 68) 25px 25px 0px, rgb(34, 67, 68) 26px 26px 0px, rgb(34, 68, 69) 27px 27px 0px, rgb(35, 69, 70) 28px 28px 0px, rgb(35, 69, 70) 29px 29px 0px, rgb(35, 70, 71) 30px 30px 0px, rgb(36, 70, 71) 31px 31px 0px, rgb(36, 71, 72) 32px 32px 0px, rgb(36, 72, 73) 33px 33px 0px, rgb(37, 72, 73) 34px 34px 0px; font-size: 80px; color: rgb(255, 255, 255); height: 150px; width: 150px; line-height: 150px; border-radius: 0%; text-align: center; background-color: rgb(37, 73, 74);"> GRS </p>
               </div>
           </li><br>
           <!--/. Logo -->
           <!--Social-->
           <li>
               <ul class="social">
                   <li><a class="icons-sm fb-ic"><i class="fa fa-facebook"> </i></a></li>
                   <li><a class="icons-sm pin-ic"><i class="fa fa-pinterest"> </i></a></li>
                   <li><a class="icons-sm gplus-ic"><i class="fa fa-google-plus"> </i></a></li>
                   <li><a class="icons-sm tw-ic"><i class="fa fa-twitter"> </i></a></li>
               </ul>
           </li>
           <!--/Social-->
           <!--Search Form-->
           <li>
               <form class="search-form" role="search">
                   <div class="form-group waves-light">
                       <input type="text" class="form-control" placeholder="Search">
                   </div>
               </form>
           </li>
           <!--/.Search Form-->
           <!-- Side navigation links -->
           <li>
               <ul class="collapsible collapsible-accordion">
                   <li>
                     <a class="collapsible-header waves-effect arrow-r">
                       <i class="fa fa-pencil faa-horizontal animated-hover"></i> Blog<i class="fa fa-angle-down rotate-icon"></i>
                     </a>
                       <div class="collapsible-body">
                           <ul>
                               <li>
                                 <a href="/posts" class="waves-effect">Posts</a>
                               </li>
                               <% if user_signed_in? && (current_user.admin?) %>
                               <li>
                                 <a href="/posts/new" class="waves-effect">New Post</a>
                               </li>
                               <% end %>
                           </ul>
                       </div>
                   </li>
                   <li><a class="collapsible-header waves-effect arrow-r"><i class="fa fa-line-chart faa-float animated-hover"></i> Exercise<i class="fa fa-angle-down rotate-icon"></i></a>
                       <div class="collapsible-body">
                           <ul>
                              <li>
                                <a href="/exercises" class="waves-effect">Exercise</a>
                              </li>
                              <% if user_signed_in? && (current_user.admin?) %>
                              <li>
                                <%= link_to 'New Exercise', new_exercise_path(current_user), class: "waves-effect" %>
                              </li>
                              <% end %>
                           </ul>
                       </div>
                   </li>
                   <li><a  class="collapsible-header waves-effect arrow-r"><i class="fa fa-eye faa-pulse animated-hover"></i> About<i class="fa fa-angle-down rotate-icon"></i></a>
                       <div class="collapsible-body">
                           <ul>
                               <li>
                                 <a href="/pages/about" class="waves-effect">About</a>
                               </li>
                               <li>
                                 <a href="#" class="waves-effect">My Work</a>
                               </li>
                           </ul>
                       </div>
                   </li>
                   <li><a class="collapsible-header waves-effect arrow-r"><i class="fa fa-envelope-o faa-shake animated-hover"></i> Contact me<i class="fa fa-angle-down rotate-icon"></i></a>
                       <div class="collapsible-body">
                           <ul>
                               <li>
                                 <a href="/pages/faq" class="waves-effect">FAQ</a>
                               </li>
                               <li>
                                 <a href="/pages/contact" class="waves-effect">Write a message</a>
                               </li>
                           </ul>
                       </div>
                   </li>
               </ul>
           </li>
           <!--/. Side navigation links -->
           <div class="sidenav-bg mask-strong"></div>
       </ul>
       <!--/. Sidebar navigation -->
       <!-- Navbar -->
       <nav class="navbar fixed-top navbar-toggleable-md navbar-dark scrolling-navbar double-nav">
           <!-- SideNav slide-out button -->
           <div class="float-xs-left">
               <a href="#" data-activates="slide-out" class="button-collapse"><i class="fa fa-bars"></i></a>
           </div>
           <!-- Breadcrumb-->
           <div class="breadcrumb-dn mr-auto">
             &nbsp; &nbsp; &nbsp;<a href="/"><i class="fa fa-hospital-o faa-tada animated-hover fa-2x"></i></a>&nbsp; &nbsp;
             <%= link_to "Generations Rehab Services", root_path, class: 'navbar-brand' %>
           </div>
           <ul class="nav navbar-nav ml-auto flex-row">
             <li class="nav-item">
               <a class="nav-link" rel="nofollow" href="/pages/about"><i class="fa fa-eye faa-tada animated-hover"></i> <span class="hidden-sm-down">About</span></a>
             </li>
             <li class="nav-item">
               <a class="nav-link" rel="nofollow" href="/pages/faq"><i class="fa fa-question faa-tada animated-hover"></i> <span class="hidden-sm-down">FAQ</span></a>
             </li>
             <li class="nav-item">
               <a class="nav-link" rel="nofollow" href="/pages/contact"><i class="fa fa-envelope-o faa-tada animated-hover"></i> <span class="hidden-sm-down">Contact</span></a>
             </li>
            <% if user_signed_in? %>
              <li class="nav-item">

                <a href="/users/edit" class="nav-link"><i class="fa fa-user-secret faa-tada animated-hover"></i> <span class="hidden-sm-down"><%= current_user.first_name.capitalize %></span></a>
              </li>
              <li class="nav-item">
                <a class="nav-link" rel="nofollow" data-method="delete" href="/users/sign_out"><i class="fa fa-sign-out faa-tada animated-hover"></i> <span class="hidden-sm-down">Sign Out</span></a>
              </li>
            <% else %>
              <li class="nav-item">
                <a class="nav-link" data-toggle="modal" data-target="#modal-register">
                  <i class="fa fa-user-plus faa-tada animated-hover"></i> <span class="hidden-sm-down">Sign Up</span>
                </a>
              </li>
              <li class="nav-item">
                <a class="nav-link" data-toggle="modal" data-target="#modal-login"><i class="fa fa-sign-in faa-tada animated-hover"></i> <span class="hidden-sm-down">Sign In</span>
                </a>
              </li>
            <% end %>
           </ul>
       </nav>
       <!-- /.Navbar -->
   </header>
   <!--/.Double navigation-->
